{
    "eda": {
        "visualize": true,
        "save_dir": "outputs/eda_figures"
    },
    "data": {
        "file_path": "data3.csv",
        "missing_values": {
            "strategy": "fill",
            "fill_value": 0
        },
        "outliers": {
            "strategy": "cap",
            "lower_bound": 0,
            "upper_bound": 1000000
        }
    },
    "preprocessing": {
        "missing_strategy": "fill",
        "fill_value": 0,
        "outlier_strategy": "cap",
        "scaling": "standard"
    },
    "modeling": {
        "allowed_models": [
            "LinearRegression",
            "DecisionTreeRegressor"
        ],
        "tuning": {
            "enabled": true,
            "strategy": "grid",
            "param_grids": {
                "LinearRegression": {},
                "DecisionTreeRegressor": {
                    "max_depth": [
                        3,
                        5,
                        10
                    ],
                    "min_samples_split": [
                        2,
                        5
                    ]
                }
            },
            "cv": 3,
            "scoring": "r2"
        }
    }
}